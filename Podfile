platform :ios, ‘8.0‘
xcodeproj 'CommunityHospital.xcodeproj'

# ruby语法
# target数组 如果有新的target直接加入该数组
targetsArray = ['Doctors', 'Patients']

targetsArray.each do |t|
    target t do
        pod 'AFNetworking', '3.2.0'
        pod 'SDWebImage', '~> 4.3.0'
        pod 'MJRefresh', '~> 3.1.12'

        pod 'TPKeyboardAvoiding', '1.3.1'
        pod 'FFFJsonKillNull', '~> 1.1.0’
        pod 'ProgressHUD', '~> 2.51'
        pod 'JPush', '~> 3.0.6'
        pod 'Toast', '~> 4.0.0'
        pod 'WechatOpenSDK', '~> 1.8.2'



        
        pod 'RongCloudIM/IMLib', '~> 2.8.3'
        pod 'RongCloudIM/IMKit', '~> 2.8.3'
#    pod 'RongCloudRTC/RongCallLib', '2.9.0'
#    pod 'RongCloudRTC/RongCallKit', '2.9.0'
    end
end

